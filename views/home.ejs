<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="../css/style.css">
        <script src="../script/home.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <title>Smart Recruiter</title>
        <script>
            function handle(){
                var temp1='<%= user%>';
                if(temp1.length>8){
                    temp1=temp1.substr(0,7)+"...";
                    document.getElementById("username").innerHTML=temp1;
                }
                
                var temp='<%= flag%>';
                if(temp==1){
                    login_modal();
                }
                else if(temp==2){
                    signup_modal();
                }
                // console.log(window.innerWidth)
                window.onresize = function(){
                    // console.log(window.innerWidth);
                    if(window.innerWidth<550 && window.innerWidth>390){
                        document.getElementById("company-name").innerHTML="S-Recruiter"
                    }
                    else if(window.innerWidth<390){
                        document.getElementById("company-name").innerHTML="S R"
                    }
                    else{
                        document.getElementById("company-name").innerHTML="Smart Recruiter"
                    }
                }
            }
        </script>
    </head>
    <body onload="handle()" class="wallpaper">
        <div class="modal" id="modal-login">
            <div class="forms-container">
                <form class="forms-container-item" action="/auth/login" method="POST" enctype="application/json">
                    <span class="forms-item forms-brand" for=""><a class="nav-bar-link" href="javascript:close()">Back</a>Login</span>
                    <input class="forms-item" type="text" placeholder="Email" name="email" required>
                    <input class="forms-item" type="password" placeholder="Password" name="psw" required>
                    <% if( status ) { %>
                    <span class="forms-item hidden-status"><%= status %></span>
                    <% } %>
                    <button class="forms-item forms-btn" type="submit" >LOGIN</button>
                    <label class="forms-item" for=""><a class="forms-link" href="#">Forgot Password ?</a></label>
                    <label class="forms-item" for="">New User? <a class="forms-link" href="javascript:switchToSignup()">Signup</a></label>
                    <div class="forms-item">Hidden</div>
        
                </form>
            </div>
        </div>
        <div class="modal" id="modal-signup">
            <div class="forms-container">
                <form class="forms-container-item" action="/auth/signup" method="POST" enctype="application/json">
                    <label class=" forms-item forms-brand" for=""><a class="nav-bar-link" href="javascript:close()">Back</a>SignUp</label>
                    <input class="forms-item" type="text" placeholder="Username *" name="uname" required>
                    <input class="forms-item" type="text" placeholder="Email ID *" name="email" required>
                    <input class="forms-item" type="password" placeholder="Password *" name="psw" required>
                    <input class="forms-item" type="password" placeholder="Re-Enter Password *" name="psw" required>
                    <% if( status ) { %>
                        <span class="forms-item hidden-status"><%= status %></span>
                    <% } %>
                    <button class="forms-item forms-btn" type="submit" >SIGN UP</button>
                    <div class="forms-item">Hidden</div>
                </form>
            </div>
        </div>
        <nav class="navbar">
            <div class="nav-brand">
                <a class="nav-bar-link" href="/" id="company-name">Smart Recruiter</a>
            </div>
            <% if( user ){ %>
                <div class="nav-primary-btn">
                    <a class="nav-btn nav-bar-link" id="username" href="/profile">User</a>
                </div>
                <div class="nav-primary-btn">
                    <a class="nav-btn nav-bar-link" href="/auth/logout">Logout</a>
                </div>
            <% } 
            else{ %>
                <div class="nav-primary-btn">
                    <button class="nav-btn" onclick="login_modal()">Login</button>
                </div>
                <div class="nav-primary-btn">
                    <button class="nav-btn" onclick="signup_modal()">SignUp</button>
                </div>
            <% } %>
        </nav>
